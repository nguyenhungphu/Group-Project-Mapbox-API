k.eyJ1Ijoibmd1eWVuaHVuZ3BodTc3NyIsImEiOiJja2psanoxOXAwemg5MnFxeXQyMHV6M2s4In0.Mmge4RRo03t0OWuVuHSdKQ\"\r\nconst token = 'pk.eyJ1Ijoibmd1eWVuaHVuZ3BodTc3NyIsImEiOiJja2psanoxOXAwemg5MnFxeXQyMHV6M2s4In0.Mmge4RRo03t0OWuVuHSdKQ';\r\n\r\nlet search = document.querySelector('#search')\r\n\r\nsearch.onsubmit = e => {\r\n  input = e.target.querySelector('input')\r\n  console.log(input)\r\n  getLocation();\r\n  e.preventdefault();\r\n}\r\n\r\nfunction getLocation(location){\r\n  if ('geolocation' in navigator) {\r\n    navigator.geolocation.getCurrentPosition(position => {\r\n      success(position.coords.longitude, position.coords.latitude);\r\n      forwardGeoCoding(location);\r\n\r\n    })\r\n  }\r\n}\r\n\r\nfunction renderPOI(){}\r\n\r\nfunction forwardGeoCoding(location) {\r\n  fetch(`https://api.mapbox.com/geocoding/v5/mapbox.places/${location}.json?&types=poi&access_token=pk.eyJ1Ijoibmd1eWVuaHVuZ3BodTc3NyIsImEiOiJja2psanoxOXAwemg5MnFxeXQyMHV6M2s4In0.Mmge4RRo03t0OWuVuHSdKQ`)\r\n  .then(resp => {return resp.json()})\r\n  .then(data => {console.log(data)})\r\n  .catch(err => {console.log(err)})\r\n}\r\n\r\nfunction success(lon, lat) {\r\n  mapboxgl.accessToken = token;\r\n  let map = new mapboxgl.Map({\r\n    container: 'map',\r\n    style: 'mapbox://styles/mapbox/streets-v11', // stylesheet location\r\n    center: [lon, lat], // starting position [lng, lat]\r\n    zoom: 15 // starting zoom\r\n  });\r\n\r\n  let marker = new mapboxgl.Marker()\r\n  .setLngLat([lon, lat])\r\n  .addTo(map);\r\n\r\n}\r\n\r\nwindow.addEventListener('load', getLocation);\r\ngetLocation();"},"lineCount":null}},"error":null,"hash":"40670ea065c6474879dfbf322bdb3c02","cacheData":{"env":{}}}